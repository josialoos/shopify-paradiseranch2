{
  "name": "paradiseranch2",
  "version": "3.0.0",
  "description": "This is the Paradise Ranch Website 3.0",
  "main": "index.js",
  "author": "Josia Loos",
  "license": "MIT",
  "scripts": {
    "start": "concurrently 'ttab yarn sass' 'ttab yarn themewatch' 'ttab yarn browsersync'",
    "sass": "sass --watch assets/scss:assets/css",
    "themewatch": "theme watch --allow-live --notify=/tmp/theme.update",
    "browsersync": "browser-sync start --proxy 'https://paradiseranch2.myshopify.com' --files '/tmp/theme.update' --reload-delay 1500 --config bs-config.js"
  },
  "dependencies": {
    "browser-sync": "^2.26.14",
    "concurrently": "^5.3.0",
    "dart-sass": "^1.25.0",
    "sass": "^1.32.6",
    "themekit": "^2.1.0",
    "ttab": "^0.7.0",
    "webpack": "^5.21.2"
  }
}
